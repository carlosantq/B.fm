IMPLEMENTATION GerenciadorTag_i
REFINES GerenciadorTag

SEES
 Tag_Ctx


OPERATIONS
 cadastrar_tag(tt) = 
 PRE
	tt : TAG & tt /: tags
 THEN 
	tags := tags \/ { tt }
 END;


 remover_tag(tt) = 
 PRE 
	tt : TAG & tt : tags
 THEN 
	tags := tags - { tt }
 END;




 END