IMPLEMENTATION GerenciadorTag_i
REFINES GerenciadorTag

SEES
 Tag_Ctx

CONCRETE_VARIABLES
    tags_i

INVARIANT
    tags_i : TAG --> BOOL &
    dom(tags_i |> {TRUE}) = tags
    
INITIALISATION
    tags_i := TAG * {FALSE}
    
OPERATIONS
  cadastrar_tag(tt) =
	tags_i(tt) := TRUE;

  remover_tag(tt) =
	tags_i(tt) := FALSE;

  rr <-- buscar_tag(tt) =
	rr := tags_i(tt);

 tt <-- listar_tags = 
    tt := tags_i

 END