IMPLEMENTATION GerenciadorMusica_i
REFINES GerenciadorMusica

SEES 
 Musica_Ctx


OPERATIONS
 cadastrar_musica(mm) =
 PRE
	mm : MUSICA & mm /: musicas
 THEN
	musicas := musicas \/ { mm }
 END;


 remover_musica(mm) =
 PRE
 	mm : MUSICA & mm : musicas
 THEN
	musicas := musicas - { mm }
 END;




 END